<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <CNTKDllDir>$(MSBuildThisFileDirectory)\..\..\support\x64\</CNTKDllDir>
  </PropertyGroup>
  <ItemGroup Condition="'$(Platform.ToLower())' == 'x64'">
    <None Include="$(CNTKDllDir)Release\Cntk.Eval-2.0.dll">
      <Link>Cntk.Eval-2.0.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Release\Cntk.Math-2.0.dll">
      <Link>Cntk.Math-2.0.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Dependency\libiomp5md.dll">
      <Link>libiomp5md.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(CNTKDllDir)Dependency\mkl_cntk_p.dll">
      <Link>mkl_cntk_p.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>